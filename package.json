{
  "name": "card-game-app-server",
  "version": "0.0.1",
  "description": "GI-like online card game",
  "engineStrict": true,
  "engines": {
    "node": "10.14.0"
  },
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:debug:docker": "nodemon --config nodemon-debug-docker.json",
    "start:staging": "NODE_ENV=staging npm run start:debug",
    "start:prod": "NODE_ENV=production ./node_modules/.bin/forever -m 10 ./build/src/main.js",
    "docker:up": "docker-compose up -d --force-recreate",
    "watch": "nodemon -e ts -w ./src -x npm run start",
    "clean": "rimraf coverage build tmp",
    "db:seed:sqlite": "NODE_ENV=sqlite npm run db:seed",
    "db:seed": "ts-node seeders/seed.ts",
    "db:seed:gcp": "NODE_ENV=gcp ts-node seeders/seed.ts",
    "db:clear": "ts-node seeders/clear.ts",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "NODE_ENV=test jest --coverage --runInBand",
    "test:sqlite": "NODE_ENV=sqlite jest --coverage --runInBand",
    "test:watch": "jest --watch --runInBand"
  },
  "dependencies": {
    "@firebase/auth": "^0.12.0",
    "@types/body-parser": "^1.16.8",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.11.1",
    "@types/express-validator": "^3.0.0",
    "@types/log4js": "^2.3.5",
    "@types/morgan": "^1.7.37",
    "@types/request-promise": "^4.1.41",
    "@types/request-promise-native": "^1.0.17",
    "@types/socket.io": "^1.4.36",
    "@types/socket.io-client": "^1.4.32",
    "bitcoinjs-lib": "^3.3.2",
    "body-parser": "^1.18.2",
    "config": "^1.30.0",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "express-graceful-shutdown": "^1.0.0",
    "express-namespace": "^0.1.1",
    "express-validator": "^5.1.1",
    "firebase": "^6.6.2",
    "firebase-admin": "^11.4.1",
    "forever": "^1.0.0",
    "helmet": "^3.21.1",
    "javascript-state-machine": "^3.1.0",
    "log4js": "^3.0.6",
    "morgan": "^1.9.1",
    "mysql": "^2.15.0",
    "mysql2": "^1.7.0",
    "node": "^10.16.3",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.7",
    "sequelize": "^4.44.3",
    "sequelize-typescript": "^0.6.10",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "@types/express-session": "^1.15.14",
    "@types/jest": "^23.3.14",
    "@types/node": "^9.4.6",
    "@types/supertest": "^2.0.4",
    "express-session": "^1.16.2",
    "jest": "~23.6.0",
    "prettier": "1.14.3",
    "rimraf": "~2.6.2",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^5.0.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-microsoft-contrib": "~5.2.1",
    "tsutils": "~3.0.0",
    "typescript": "^3.2.2"
  },
  "author": "Naoto Satoh<stardustforeve012@gmail.com>",
  "license": "Apache-2.0"
}
